{
  "name": "appahouse-portfolio",
  "version": "1.0.0",
  "description": "A minimalist portfolio website showcasing full-stack Tanstack Start apps and Docker-deployed applications",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss -i ./css/input.css -o ./css/output.css --watch",
    "build": "tailwindcss -i ./css/input.css -o ./css/output.css",
    "dev": "node start-dev.js",
    "dev:css": "npm run build:css",
    "dev:server": "node dev-server.js",
    "start": "npm run build",
    "blog:new": "node scripts/blog-generator.js new",
    "blog:from-code": "node scripts/blog-generator.js from-code",
    "blog:list": "node scripts/blog-generator.js list",
    "blog:quick-md": "node scripts/create-blog.js from-md",
    "blog:quick-code": "node scripts/create-blog.js from-code",
    "blog:batch": "node scripts/create-blog.js batch",
    "blog:update-index": "node scripts/update-blog-index.js",
    "project:new": "node scripts/project-generator.js new",
    "project:list": "node scripts/project-generator.js list",
    "project:update:all": "node scripts/update-projects.js all",
    "project:update:single": "node scripts/update-projects.js single",
    "case-study:new": "node scripts/case-study-generator.js new",
    "case-study:list": "node scripts/case-study-generator.js list",
    "case-study:update": "node scripts/case-study-generator.js update"
  },
  "keywords": [
    "portfolio",
    "tanstack",
    "docker",
    "full-stack"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0"
  }
}
